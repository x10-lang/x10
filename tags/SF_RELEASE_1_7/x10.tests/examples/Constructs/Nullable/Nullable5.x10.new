import harness.x10Test;;

/**
 * Casting nullable T to T when the value is null
 * should not cause a class cast exception
 * if T is itself nullable S.
 *
 * nullable nullable T should be treated as identical to nullable T.
 *
 * @author kemal, 12/2004
 */
public class Nullable5 extends x10Test {
	public def run(): boolean = {
		var gotNull: boolean = false;
		try {
			var x: nullable<nullable<Nullable5>> = X.mynull();
			var y: nullable<Nullable5> = (nullable<Nullable5>) x;
			// x and y are of the same type
			if (x == y) X.use(y);
			// y and use2 argument are of the same type
			X.use2(y);
		} catch (var e: ClassCastException) {
			gotNull = true;
		}
		return !gotNull;
	}

	public static def main(var args: Rail[String]): void = {
		new Nullable5().execute();
	}

	static class X {
		public static def use(var y: nullable<Nullable5>): void = { }
		public static def use2(var y: nullable<nullable<nullable<Nullable5> > >): void = { }
		public static def mynull(): nullable<nullable<Nullable5> > = { return null; }
	}
}
